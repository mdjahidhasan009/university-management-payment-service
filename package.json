{
  "name": "university-management-payment-service",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "",
  "author": "Programming Hero",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' -T ./src/server.ts",
    "build": "rimraf dist && tsc",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist",
    "buildServer": "node ./dist/server.js",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint-prettier": "yarn lint:check && yarn prettier:check",
    "husky-install": "husky install",
    "husky-prepare-pre-commit": "husky add .husky/pre-commit 'yarn lint-staged'",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:generate": "npx prisma generate",
    "postinstall": "prisma generate"
  },
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
  ],
  "dependencies": {
    "@prisma/client": "^4.15.0",
    "axios": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "http": "^0.0.1-security",
    "http-status": "^1.6.2",
    "jsonwebtoken": "^9.0.0",
    "redis": "^4.6.6",
    "util": "^0.12.5",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs": "^0.0.1-security",
    "nodemon": "^2.0.22",
    "path": "^0.12.7",
    "prettier": "^2.8.8",
    "prisma": "^4.15.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "pre-commit": "^1.2.2"
  }
}
